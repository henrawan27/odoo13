<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <function model="extra.company.settings" name="get_company_settings"/>

        <record model="res.config.settings" id="res_config_settings_mpn">
            <field name="tax_exigibility" eval="1"/>
            <field name="use_custom_cash_basis_taxes" eval="1"/>
            <field name="report_minus_bracket" eval="1"/>
            <field name="compute_asset_based_opening" eval="1"/>
            <field name="totals_below_sections" eval="0"/>
            <field name="sale_tax_id" ref="mpn_account_fix.tax_vat_10_out"/>
            <field name="purchase_tax_id" ref="mpn_account_fix.tax_vat_10_in"/>
        </record>

        <function model="res.config.settings" name="execute">
            <value eval="[ref('res_config_settings_mpn')]"/>
        </function>
    </data>
</odoo>